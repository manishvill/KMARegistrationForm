package kma_registration_form_DB;


public class InputPayment {

	public static String regid;
	public static String paymentMode = null;
	public static String reciptNo = null;
	public static String transactionNo = null;
	public static String chqddNo = null;
	public static String amountp = null;
	public static String amountb = null;
	
	
	
	public static String getChqddNo() {
		return chqddNo;
	}


	public static void setChqddNo(String chqddNo) {
		InputPayment.chqddNo = chqddNo;
	}
	
	public static int getRegid() {
		return regid;
	}

	public static void setRegid(int regid) {
		InputPayment.regid = regid;
	}

	public static String getPaymentMode() {
		return paymentMode;
	}

	public static void setPaymentMode(String paymentMode) {
		InputPayment.paymentMode = paymentMode;
	}

	public static String getReciptNo() {
		return reciptNo;
	}

	public static void setReciptNo(String reciptNo) {
		InputPayment.reciptNo = reciptNo;
	}

	public static String getTransactionNo() {
		return transactionNo;
	}

	public static void setTransactionNo(String transactionNo) {
		InputPayment.transactionNo = transactionNo;
	}

	public static String getAmountp() {
		return amountp;
	}

	public static void setAmountp(String amount) {
		InputPayment.amountp = amount;
	}

	public static String getAmountb() {
		return amountb;
	}

	public static void setAmountb(String amount) {
		InputPayment.amountb = amount;
	}
}
